    <dict>
      <key>NSAllowsArbitraryLoads</key>
      <false />
      <key>NSAllowsLocalNetworking</key>
      <true />
    </dict>
    <!-- Permissions Start Here -->
    <key>NSPhotoLibraryUsageDescription</key>
    <string>$(PRODUCT_NAME) requires access to your photo library to allow you to upload photos and
      create custom signs. We do not store or use your photos for any other purpose.</string>
    <key>NSCameraUsageDescription</key>
    <string>$(PRODUCT_NAME) requires access to your photo library to allow you to upload photos and
      create custom signs. We do not store or use your photos for any other purpose.</string>
    <key>NSMicrophoneUsageDescription</key>
    <string>Give $(PRODUCT_NAME) permission to use your microphone. Your record wont be shared
      without your permission.</string>
    <key>locationAlwaysAndWhenInUsePermission</key>
    <string>$(PRODUCT_NAME) collects location data to provide personalized content and improve
      user experience. Your location data is not shared with third parties without your consent.</string>
    <key>NSLocationWhenInUseUsageDescription</key>
    <string>$(PRODUCT_NAME) collects location data to provide personalized content and improve
      user experience. Your location data is not shared with third parties without your consent.</string>
    <!-- Permissions End Here -->
    <key>NSUserActivityTypes</key>
    <array>
      <string>$(PRODUCT_BUNDLE_IDENTIFIER).expo.index_route</string>
    </array>
    <key>RCTNewArchEnabled</key>
    <true />
